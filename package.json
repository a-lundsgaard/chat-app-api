{
  "name": "chat-app-dis",
  "packageManager": "yarn@3.2.2",
  "scripts": {
    "mac:copy:sql": "cd ./dist/src/database && mkdir -p sql && cd .. && cd .. && cd .. && cp -R ./src/database/sql ./dist/src/database",
    "win:copy:sql": "cd .\\dist\\src\\database && mkdir sql && cd ..\\..\\.. && copy .\\src\\database\\sql .\\dist\\src\\database\\sql",
    "dev": "npx tsc && nodemon ./dist/server.js",
    "dev:mac": "npx tsc && yarn mac:copy:sql && nodemon ./dist/server.js",
    "dev:win": "npx tsc && yarn win:copy:sql && nodemon ./dist/server.js",
    "dev-concurrent": "concurrently \"yarn dev\" \"npx tsc --watch\"  \"node ./dist/watcher\" -k",
    "create-tables": "node ./dist/src/database/actions/setupdb.js",
    "start": "node dist/server.js",
    "codegen": "graphql-codegen --config codegen.ts",
    "build": "tsc"
  },
  "dependencies": {
    "@apollo/server": "^4.7.1",
    "@graphql-tools/schema": "^9.0.19",
    "@types/express": "^4.17.17",
    "@types/graphql": "^14.5.0",
    "@types/pg": "^8.6.6",
    "apollo-server-express": "^3.12.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cookie": "^0.5.0",
    "cookies": "^0.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.13.0",
    "jsonwebtoken": "^9.0.0",
    "nodemon": "^2.0.22",
    "pg": "^8.10.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-resolvers": "3.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/cookies": "^0.7.7",
    "@types/jsonwebtoken": "^9.0.2",
    "concurrently": "^8.0.1"
  }
}